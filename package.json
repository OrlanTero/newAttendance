    "@mui/x-date-pickers": "^7.28.0",
    "@progress/kendo-drawing": "^1.21.2",
    "@progress/kendo-react-pdf": "^10.0.0",
    "axios": "^1.8.3",
    "body-parser": "^1.20.3",
    "chart.js": "^4.4.8",
    "cors": "^2.8.5",

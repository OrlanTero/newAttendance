    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "usb": "^2.15.0"

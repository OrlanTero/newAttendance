{
  "name": "attendanceelectron",
  "productName": "attendanceelectron",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "postinstall": "electron-rebuild",
    "build-electron": "electron-builder",
    "postbuild": "cd api && npm start && cd ../FingerprintAPI/FingerPrintA/FingerPrintA/bin/Debug/net8.0-windows && FingerPrintA.exe"
  },
  "build": {
    "appId": "com.attendanceelectron.yourapp",
    "productName": "Attendance Electron",
    "asarUnpack": "**/*.node",
    "win": {
      "target": "nsis",
      "signAndEditExecutable": false
    },
    "files": [
      "src/**/*",
      "src/index.js"
    ]
  },
  "keywords": [],
  "author": {
    "name": "Orlan",
    "email": "jhonorlantero@gmail.com"
  },
  "license": "MIT",
